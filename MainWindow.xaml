        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:txtmsgTranslateUnit2"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="txtImput" AcceptsReturn="True"></TextBox>
        <Button x:Name="txtTranslate" Content="Click to Tranlsate Text Message!" Click="txtTranslate_Click"></Button>
        <Label x:Name="lblOutput"></Label>
    </StackPanel>
</Window>
